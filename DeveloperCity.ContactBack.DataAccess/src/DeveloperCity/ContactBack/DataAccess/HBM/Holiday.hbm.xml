<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="DeveloperCity.ContactBack.DomainModel.Holiday" table="`Holiday`">
        <id column="`HolidayID`" name="HolidayID" type="long" unsaved-value="0">
            <generator class="identity"/>
        </id>
        <timestamp access="field" column="`ModifiedOn`" name="ModifiedOn"/>
        <property column="`HolidayName`" length="100" name="HolidayName" not-null="true" type="java.lang.String"/>
        <property column="`Day`" name="Day" not-null="true" type="java.util.Date"/>
        <property access="field" column="`CreatedOn`" name="CreatedOn" not-null="true" type="java.util.Date"/>
    </class>
</hibernate-mapping>
