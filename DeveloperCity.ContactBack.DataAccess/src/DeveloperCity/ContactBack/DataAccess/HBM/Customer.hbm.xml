<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <joined-subclass extends="DeveloperCity.ContactBack.DomainModel.User" lazy="false" name="DeveloperCity.ContactBack.DomainModel.Customer" table="`Customer`">
        <key column="`UserID`"/>
        <property column="`MobilePhone`" length="10" name="MobilePhone" not-null="false" type="java.lang.String"/>
    </joined-subclass>
</hibernate-mapping>
