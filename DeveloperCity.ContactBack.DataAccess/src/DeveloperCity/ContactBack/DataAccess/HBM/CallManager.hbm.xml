<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="DeveloperCity.ContactBack.DomainModel.CallManager" table="`CallManager`">
    <id column="`CallManagerID`" name="CallManagerID" type="long" unsaved-value="0">
      <generator class="identity"/>
    </id>
    <timestamp access="field" column="`ModifiedOn`" name="ModifiedOn"/>
    <property column="`IP`" length="31" name="IP" not-null="true" type="java.lang.String"/>
    <property column="`JtapiUsername`" length="20" name="JtapiUsername" not-null="true" type="java.lang.String"/>
    <property column="`JtapiPassword`" length="128" name="JtapiPassword" not-null="true" type="java.lang.String"/>
    <property access="field" column="`CreatedOn`" name="CreatedOn" not-null="true" type="java.util.Date"/>
  </class>
</hibernate-mapping>
