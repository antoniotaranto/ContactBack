<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="DeveloperCity.ContactBack.DomainModel.Module" table="`Module`">
    <id column="`ModuleID`" name="ModuleID" type="long" unsaved-value="0">
      <generator class="identity"/>
    </id>
    <timestamp access="field" column="`ModifiedOn`" name="ModifiedOn"/>
    <property column="`Description`" length="50" name="Description" not-null="true" type="java.lang.String"/>
    <property column="`Servlet`" length="100" name="Servlet" not-null="true" type="java.lang.String"/>
    <property access="field" column="`CreatedOn`" name="CreatedOn" not-null="true" type="java.util.Date"/>
    <property column="`Portal`" name="Portal" type="DeveloperCity.ContactBack.DataAccess.UserTypes.Portal"/>
  </class>
</hibernate-mapping>
