<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="DeveloperCity.ContactBack.DomainModel.Chart" table="`Chart`">
    <id column="`ChartID`" name="ChartID" type="long" unsaved-value="0">
      <generator class="identity"/>
    </id>
    <timestamp access="field" column="`ModifiedOn`" name="ModifiedOn"/>
    <property access="field" column="`CreatedOn`" name="CreatedOn" not-null="true" type="java.util.Date"/>
    <property column="`ChartFile`" length="45" name="ChartFile" not-null="true" type="java.lang.String"/>
    <property column="`ChartDescription`" length="65" name="ChartDescription" not-null="true" type="java.lang.String"/>
  </class>
</hibernate-mapping>
