body.home div.outer div.limit div.inside div.content form fieldset legend span b{
	margin: 0 6px 6px 6px;
	color: #FFFFFF;
	height:41px;
	width:360px;
	display:block;
	text-align:center;
	padding-top:10px;

}



body.home div.outer div.limit div.inside div.content  {
	margin:0;
}

body.home div.outer div.limit div.inside div.content form fieldset ul.login {
	background:url(../IMAGES/bg-login.png) no-repeat;
	height:131px;
	width:372px;
	display:block;
	padding: 31px 0 0 62px;
}

body.home div.outer div.limit div.inside div.content form fieldset ul.login li {
	margin-bottom:20px;
}

body.home div.outer div.limit div.inside div.content form fieldset ul.login li label {
	width:48px;
	display:block;
	float:left;
}

body.home div.outer div.limit div.inside div.content form fieldset ul.login li input {
	height:17px;
	border:1px solid #dedede;
	width:164px;
}

body.home div.outer div.limit div.inside div.content form fieldset.buttons {
	width:100%;
	text-align:right;
	margin-left:-5px;
}

body.home div.outer div.limit div.inside div.content ul.welcome {
	margin:70px 0 0 180px;
}

body.home div.outer div.limit div.inside div.content ul.welcome li {
	background:url(../IMAGES/bg-box-home-top.png) no-repeat;
	float:left;
	width:197px;
	margin-right:15px;
}


body.home div.outer div.limit div.inside div.content ul.welcome li strong{
	height: 37px;
	width:187px;
	display:block;
	color:#FFFFFF;
	text-align:center;
	padding-top:7px;
	margin:6px 5px 7px 5px;
}

body.home div.outer div.limit div.inside div.content ul.welcome li p{
	background:url(../IMAGES/bg-box-home.png) no-repeat;
	height: 164px;
	width:197px;
	margin:0;
	padding: 8px 15px 0 15px;
}

body.home div.outer div.limit div.inside div.content ul.welcome li p span.entrar{
	background:url(../IMAGES/bt-entrar-1.png) no-repeat;
	height: 22px;
	width:82px;
	margin:0;
	display:block;
	text-indent:-999999px;
	margin:10px 0 0 80px;
}

body.home div.outer div.limit div.inside div.content ul.welcome li p span.entrar a {
	display:block;
	height: 22px;
	width:82px;
}




/* START FOOTER */
div.outer div.footer {
    width: 100%;    
    text-align: center;
	clear:both;    
	display:block;
	position:relative;	
	background:url(../IMAGES/bg-footer.gif) repeat-x;
	height:54px;
}

div.outer div.footer p.direitos {
	padding:14px 0 0 0;
	color: #4c591c;
	size: 11px;	
	background: url(../IMAGES/bg-footer-inside.gif);
	height:54px;
	width:1003px;
	display:block;
	margin: 0 auto;
	
}

div.outer div.footer div.logo {
	padding:0 0 0 0;
	color: #545454;
	size: 11px;	
	background: #f2f2f2;
	height:30px;
	width:100% !important;
	display:block;
}

div.outer div.footer div.logo p{
	width:200px;
	float:right;
}

div.outer div.footer div.logo span.company{
	background:url(../IMAGES/logo-company-rodape.gif) no-repeat;
	height:24px;
	width:55px;
	margin-right:20px;
	text-indent:-9999999px;
	position:absolute;
}


div.outer div.footer a {
	background:url(../IMAGES/assinatura-just.gif) no-repeat;
	width:137px;
	height:7px;
	text-indent:-999999px;
	display:block;
	margin-top:15px;
	float:right;
	margin-right:10px;
}


/* FOMULARIO */








span.ampliar {
	background: url(../IMAGES/bt-ampliar.gif) no-repeat;
	width:82px;
	height:21px;
	display:block;
	text-indent:-9999px;
}

span.ampliar a {
	width:82px;
	height:21px;
	display:block;
}


















/* START POP */
a.pop-close {
	display: block;
	background:url(../IMAGES/bt-fechar.gif) no-repeat;
	height:22px;
	width:68px;
	text-indent:-99999px;
	float:left;

}

div.pop-geral {
	background-color:#fff;
	border:3px solid #454545;
	width:653px;
}

div.pop-geral div.inside {
	background-color:#fff;
	width: 643px;
	padding: 5px;
	position: relative;
	height: 500px;
	padding-top: 20px;
}

div.pop-geral div.inside h1 {
	color: #fff;
	background-color:#4a4a4a;
	width:560px;
	height:22px;
	font-size:1em;
	margin-right:3px;
	padding: 0;
	text-align: left !important;
	float:left;
	padding: 4px 0 0 5px;
	margin-bottom:30px;

	
}



div.pop-geral div.inside p {
	color: #4a4a4a;
	text-align: left !important;
	font-size:1em;
	width:100%;
	clear:both;
	margin: 5px 20px 15px 5px;
}

