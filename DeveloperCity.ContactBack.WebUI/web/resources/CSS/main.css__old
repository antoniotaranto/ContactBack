@import "colors-scheme-default.css";

* {
	margin:0;
	padding:0;
	list-style:none;
	vertical-align:baseline;
	zoom: 1;
    -moz-box-sizing: border-box;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}

img {
	border: 0;
}

body {
	font:11px "Trebuchet MS", Arial, Helvetica, sans-serif;
	color: #000;
	background: url(../IMAGES/bg-body.gif) repeat-x;
	outline: none;
	-moz-outline: none;
	overflow-y: scroll;
	*overflow-y: none;
	*overflow-x: none;
	visibility:hidden;
}

body.home {
	font:11px "Trebuchet MS", Arial, Helvetica, sans-serif;
	color: #000;
	background: url(../IMAGES/bg-body-home.gif) repeat-x;
	outline: none;
	-moz-outline: none;
	overflow-y: scroll;
	*overflow-y: none;
	*overflow-x: none;
	visibility:hidden;
}

div.outer {
	position: relative;
	width:100%;
	min-width: 1024px;
}

body.home div.outer div.limit {
	display: block;
	position: relative;
	margin: 0 auto;
	width: 1003px;
}

div.outer div.limit {
	display: block;
	position: relative;
	margin: 0 auto;
	width: 100%;
}










/* START HEADER */
div.outer div.limit div.header {
	position:relative;
	height: 102px;
}

div.outer div.limit div.header div.outras-aplicacoes {
	height:38px;
	margin-bottom:5px;
	background:url(../IMAGES/bg-menu-aplicacoes.gif) left 31px repeat-x;
}

div.outer div.limit div.header div.outras-aplicacoes strong {
	float:left;
	font-weight:bold;
	color:#FFFFFF;
	padding: 9px 10px 0 10px;
	height:22px;
}

div.outer div.limit div.header div.outras-aplicacoes ul {
	height:31px;
}

div.outer div.limit div.header div.outras-aplicacoes ul li {
	float:left;
	color:#FFFFFF;
	margin-right:10px;
	margin-top:3px;
	height:23px;
	padding: 5px 10px 0 10px;
}

div.outer div.limit div.header div.outras-aplicacoes ul li.active {
	background-color: #f2f2f2;
	display:block;
	height:35px;
	padding: 5px 10px 0 10px;
}

div.outer div.limit div.header div.outras-aplicacoes ul li.active a {
	font-weight:bold;
	text-decoration:none;
}



div.outer div.limit div.header div.outras-aplicacoes ul li a{
	color:#FFFFFF;
}

div.outer div.limit div.header ul.menu {
	display:block;
	position:absolute;
	right:10px;
}

div.outer div.limit div.header ul.menu li{
	float:left;
	margin-right:10px;
}


div.outer div.limit div.header div.logos {
	margin-left:15px;
	margin-bottom:20px;
	float:left;
	margin-top:35px;
}

div.outer div.limit div.header div.logos p{
	float:left;
}

div.outer div.limit div.header div.logos p strong {
	color:#FFFFFF;
	padding:3px 5px;
	font-size: 1.5em;
}

div.outer div.limit div.header div.logos p span {
	display:block;
	margin: 5px 0 0 0;
}

div.outer div.limit div.header div.logos h1 {
	float: left;
	margin-left:20px;
}

div.outer div.limit div.header div.logos h1 a{
	height:45px;
	width:99px;
	text-indent:-999999px;
	display:block;
}

div.outer div.limit div.header p.bem-vindo {
	background: #e9e9e9 url(../IMAGES/seta-bem-vindo.gif) right top no-repeat;
	padding:8px 22px 9px 19px;
	height: 33px;
	float:right;
	margin-right:15px;
	margin-top:35px;
	color:#8a8a8a;
}
/* END HEADER*/















div.outer div.limit div.inside {
	margin-top:50px;
}

div.outer div.limit div.inside:after {
    content: ".";
    display: block;
    clear: both;
    visibility: hidden;
}









/* START BREADCRUMB  */
div.outer div.limit div.inside div.breadcrumb {
	display:block;
	height: 41px;
	margin: 0 15px 0 15px;
	background: #f2f2f2 url(../IMAGES/breadcrumb-bottom-bg.gif) left bottom repeat-x;
}

div.outer div.limit div.inside div.breadcrumb h3 {
	width: 243px;
	color:#fff;
	text-transform: uppercase;
	display:block;
	float:left;
	padding: 7px 0 0 18px;
	height: 40px;
	font-size: 1.2em;
}

div.outer div.limit div.inside div.breadcrumb strong {
	display:block;
	float:left;
	padding: 11px 0 0 13px;
}

div.outer div.limit div.inside div.breadcrumb ul {
	display:block;
	float:left;
	padding: 11px 0 0 13px;
}

div.outer div.limit div.inside div.breadcrumb ul li {
	display:block;
	float:left;
	padding-right: 11px;
	margin-right: 6px;
	background: url(../IMAGES/primary-ietm-separator.gif) right 4px no-repeat;
	color: #6f2476;
}

div.outer div.limit div.inside div.breadcrumb ul li.last {
	padding-right: 0;
	margin-right: 0;
	background: transparent none;
}



/* END BREADCRUMB */
















/* START SIDEBAR LEFT */
div.outer div.limit div.inside div.sidebar.first {
    display: none;
    float: left;
    margin: 12px 0 0 15px;
    padding-bottom: 10px;
	min-height: 300px;
}

div.outer div.limit div.inside div.sidebar.first h3 {
    height: 19px;
    background-color: #ededed;
    margin: 20px 5px 12px 5px;
    color: #5a017f;
    font-weight: bold;
    padding: 6px 0 0 14px;
}

div.outer div.limit div.inside div.sidebar.first a.detail {
    display:block;
    background-color: #f6f6f6;
    margin: 10px 5px;
    color: #5a017f;
    font-weight: bold;
    padding: 3px;
	text-align: right;
}

div.sidebar.first ul.menu {
	display:block;
	list-style-type: none !important;
	list-style-position:inside !important;
	list-style: none !important;
	padding: 0 !important;
	float: none;
}

div.sidebar.first ul.menu li{
    margin: 0 !important;
    display: block;
	padding: 1px 3px 7px 8px;
	background: url(../IMAGES/sidebar-menu-item-bg.gif) left bottom no-repeat;
	list-style-type: none !important;
	list-style: none !important;
	float:left;
	clear:both;
	width: 243px;
}

div.sidebar.first ul.menu li.first {
 	padding: 0 3px 7px 8px;
}

div.sidebar.first ul.menu li.expanded {
	padding: 0 3px 7px 0;
	background: url(../IMAGES//sidebar-menu-item-bg.gif) left 24px no-repeat;
}

div.sidebar.first ul.menu li.expanded.first {
	padding: 2px 3px 7px 0;
}

div.sidebar.first ul.menu li.expanded ul{
	margin-top: 3px;
}

div.sidebar.first ul.menu li.expanded ul li {
	padding: 1px 0 7px 8px;
	font-size: 0.9em;
}

div.sidebar.first ul.menu li.expanded ul li.first {
	padding: 4px 0 7px 8px;
}

div.sidebar.first ul.menu li a {
    display: block;
    color: #545454;
	text-transform:uppercase;
	background: transparent none;
	height: 22px;
	padding: 2px 0 0 0;
	text-decoration:none;
	font-size: 1.1em;
}

div.sidebar.first ul.menu li.expanded a {
	padding: 2px 0 0 8px;
}

div.sidebar.first ul.menu li.expanded.active-trail a {
	padding: 0 0 7px 8px;
}


div.sidebar.first ul.menu li.expanded ul li a:before {
	content: "- ";	
}

div.sidebar.first ul.menu li.active a {
	background: #e9e9e9 url(../IMAGES/sidebar-menu-item-active.gif) right top no-repeat;
	padding: 2px 0 0 8px;
	font-weight: bold;	
}


div.sidebar.first ul.menu li.active-trail a {
	background: transparent none;
	padding: 2px 0 0 0;
	font-weight: bold;
}

div.sidebar.first ul.menu li.active.active-trail a {
	background: #e9e9e9 url(../IMAGES/sidebar-menu-item-active.gif) right top no-repeat;
	padding: 2px 0 0 8px;
	font-weight: bold;
}

div.sidebar.first ul.menu li.expanded.active.active-trail a {
	background: transparent none;
}

div.sidebar.first ul.menu li.active.active-trail ul li.active.active-trail a {
	background: #e9e5ed url(../IMAGES//sidebar-menu-item-active.gif) right top no-repeat;
	padding: 2px 0 0 8px;
	font-weight: bold;
}

div.sidebar.first ul.menu li.active.active-trail ul li a {
	background: transparent none;
	padding: 2px 0 0 0;
	font-weight: normal;
}

div.sidebar.first ul.menu li.expanded.active-trail ul li a {
	padding: 2px 0 0 0;
}


div.sidebar.first ul.menu li.active-trail ul li a {
	font-weight: normal;
}

div.sidebar.first ul.menu li.expanded ul li a {
	color: #6f2476;
}

div.sidebar.first ul.menu li.expanded ul li.active a {
	background: #e9e5ed url(../IMAGES//sidebar-menu-item-active.gif) right top no-repeat;
	padding: 2px 0 0 8px;
	font-weight: bold;
}


div.outer div.limit div.two-column-right div.sidebar.first {
    display: none;
}

div.outer div.limit div.two-column-left div.sidebar.first, 
div.outer div.limit div.three-column div.sidebar.first {
    display: block;
    width: 243px;
}


div.outer div.limit div.inside div.sidebar.last{
	float:right;
	margin-right:15px;
}

div.outer div.limit div.two-column-right div.sidebar.last,
div.outer div.limit div.three-column div.sidebar.last {
	display:block;
	width: 243px;
}

div.outer div.limit div.two-column-left div.sidebar.last {
	display:none;
}




