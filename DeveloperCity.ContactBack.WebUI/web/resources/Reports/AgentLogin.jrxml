	<queryString>
		<![CDATA[SELECT
	`User`.UserID,
	Name,
	DirectoryNumber,
	Terminal,
	LastCallTime,
	WorkTime.WorkTimeID,
	LoginTime,
	LogoutTime,
	BreakStart,
	BreakEnd,
	Description
FROM
	Agent
		INNER JOIN `User` ON
			`User`.UserID = Agent.UserID
		LEFT JOIN WorkTime ON
			`User`.UserID = WorkTime.UserID
		LEFT JOIN Break ON
			WorkTime.WorkTimeID = Break.WorkTimeID
		LEFT JOIN BreakType ON
			Break.BreakTypeID = BreakType.BreakTypeID
WHERE
	LoginTime BETWEEN '$P!{StartDateValue} 00:00:00' AND '$P!{EndDateValue} 23:59:59'
ORDER BY
	`User`.Name,
	WorkTime.LoginTime,
	Break.BreakStart]]>
	</queryString>
